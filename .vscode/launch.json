{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
  {
    "type": "node",
    "request": "launch",
    "name": "Launch via NPM for test",
    "runtimeExecutable": "npm",
    "runtimeArgs": [
      "test",
      "reproduction.test.js",
      "debug"
    ],
    "port": 9229,
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
    
    {
      "name": "Launch tribesBot.js",
      "type": "node",
      "console": "integratedTerminal",
      "request": "launch",
      "program": "${workspaceFolder}/tribesBot.js",
      "cwd": "${workspaceFolder}/"
    }
  ]
}